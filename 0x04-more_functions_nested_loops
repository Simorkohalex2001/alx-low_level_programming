#include "main.h"
/**
 * _isupper - evalue if letter is uppercase .
 *@c: print int
 * Return: Always 0.
 */

int _isupper(int c)
{
if (c >= 'A' && c <= 'Z')
{
return (1);
}

else
{
return (0);
}
https://github.com/em432/alx-low_level_programming/tree/master/0x04-more_functions_nested_loops
}


#include "main.h"
/**
 * _isdigit - if is a digit
 *@c: print int
 * Return: Always 0.
 */

int _isdigit(int c)
{
if (c >= 48 && c <= 57)
{
return (1);
}

else
{
return (0);
}
}
#include "main.h"
/**
 * _isdigit - if is a digit
 *@c: print int
